@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:	<http://www.w3.org/2000/01/rdf-schema#> .
@prefix mf:     <http://www.w3.org/2001/sw/DataAccess/tests/test-manifest#> .
@prefix mfx:    <http://jena.hpl.hp.com/2005/05/test-manifest-extra#> .
@prefix qt:     <http://www.w3.org/2001/sw/DataAccess/tests/test-query#> .
@prefix dawgt:  <http://www.w3.org/2001/sw/DataAccess/tests/test-dawg#> .
@prefix sd:     <http://www.w3.org/ns/sparql-service-description#> .

<>  rdf:type mf:Manifest ;
    rdfs:comment "Query tests to calculate ERT-ART" ;
    mf:entries
    ( 

:test_1
:test_2
:test_3
:test_4
:test_5
:test_10
) .

:test_1 rdf:type mf:QueryEvaluationTest ;
    mf:name    "Substract date" ;
    dawgt:approval dawgt:Approved;
#    dawgt:approvedBy <???> ;
    mf:action
         [ qt:query  <q01.rq> ;
           qt:data   <pbs.ttl> ] ;
    mf:result  <q01.srx>
    .
    
:test_2 rdf:type mf:QueryEvaluationTest ;
    mf:name    "Simple path in property" ;
    dawgt:approval dawgt:Approved;
#    dawgt:approvedBy <???> ;
    mf:action
         [ qt:query  <q02.rq> ;
           qt:data   <bdii.ttl> ] ;
    mf:result  <q02.srx>
    .
    

:test_3 rdf:type mf:QueryEvaluationTest ;
    mf:name    "Star path for RDF collection" ;
    dawgt:approval dawgt:Approved;
#    dawgt:approvedBy <???> ;
    mf:action
         [ qt:query  <q03.rq> ;
           qt:data   <bdii.ttl> ] ;
    mf:result  <q03.srx>
    .
    
    
:test_4 rdf:type mf:QueryEvaluationTest ;
    mf:name    "Filter date & order by date" ;
    dawgt:approval dawgt:Approved;
#    dawgt:approvedBy <???> ;
    mf:action
         [ qt:query  <q04.rq> ;
           qt:data   <bdii.ttl> ] ;
    mf:result  <q04.srx>
	.
	
:test_5 rdf:type mf:QueryEvaluationTest ;
    mf:name    "Filter date & order by date" ;
    dawgt:approval dawgt:Approved;
#    dawgt:approvedBy <???> ;
    mf:action
         [ qt:query  <q05.rq> ;
           qt:graphData <bdii.ttl> , <pbs.ttl>  
			] ;
    mf:result  <q05.srx>
	
    .
    
:test_10 rdf:type mf:QueryEvaluationTest ;
    mf:name    "Query to calculate ERT-ART" ;
    dawgt:approval dawgt:Approved;
#    dawgt:approvedBy <???> ;
    mf:feature sd:BasicFederatedQuery ;
    mf:action
         [ qt:query  <q10.rq> ;
           
                  qt:serviceData [
                       qt:endpoint <http://example1.org/sparql> ;
                       qt:data     <pbs.ttl>
					   ];
				  qt:serviceData [
                       qt:endpoint <http://example2.org/sparql> ;
                       qt:data     <bdii.ttl>  
					   ]
           ] ;
    mf:result  <q10.srx>
    .
